{
  "name": "vad-web",
  "type": "module",
  "version": "0.5.1",
  "description": "Voice activity detector (VAD) for the browser",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/vad-web#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/vad-web.git"
  },
  "bugs": "https://github.com/ocavue/vad-web/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*",
        "./src/index.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc --tsconfig ./tsconfig.build.json --sort documents-first --plugin typedoc-plugin-md --out temp src/index.ts && mv temp/README.md ./README.md",
    "build": "tsup && pnpm run docs",
    "test": "vitest"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.2.1",
    "comlink": "^4.4.2",
    "just-memoize": "^2.2.0",
    "p-limit": "^6.1.0",
    "recorder-audio-worklet": "^6.0.39"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.48.0",
    "tsup": "^8.3.5",
    "typedoc": "^0.27.5",
    "typedoc-plugin-md": "^0.5.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "typesVersions": {
      "*": {
        "*": [
          "./dist/*",
          "./dist/index.d.ts"
        ]
      }
    }
  }
}
