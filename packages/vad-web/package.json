{
  "name": "vad-web",
  "type": "module",
  "version": "0.4.0",
  "description": "Voice activity detector (VAD) for the browser",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/vad-web#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/vad-web.git"
  },
  "bugs": "https://github.com/ocavue/vad-web/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./vad-audio-worklet": {
      "types": "./src/vad-audio-worklet.ts",
      "import": "./src/vad-audio-worklet.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*",
        "./src/index.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc --tsconfig ./tsconfig.build.json --plugin typedoc-plugin-md --out temp src/index.ts && mv temp/README.md ./README.md",
    "build": "tsup && pnpm run docs"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.48.0",
    "fft.js": "^4.0.4",
    "tsup": "^8.3.5",
    "typedoc": "^0.27.5",
    "typedoc-plugin-md": "^0.4.3",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./vad-audio-worklet": {
        "types": "./dist/vad-audio-worklet.d.ts",
        "import": "./dist/vad-audio-worklet.js"
      }
    },
    "typesVersions": {
      "*": {
        "*": [
          "./dist/*",
          "./dist/index.d.ts"
        ]
      }
    }
  }
}
